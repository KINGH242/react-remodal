{
  "name": "react-remodal",
  "version": "1.0.0",
  "description": "A better (animated, customizable, tested etc.) modal for React",
  "keywords": [
    "react",
    "react-component",
    "react-modal",
    "react-remodal",
    "react-dialog",
    "modal",
    "remodal",
    "dialog"
  ],
  "main": "./dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tomgrooffer/react-remodal.git"
  },
  "author": "Mike Vercoelen <mike.vercoelen@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tomgrooffer/react-remodal/issues"
  },
  "homepage": "https://github.com/tomgrooffer/react-remodal#readme",
  "scripts": {
    "start": "gulp watch",
    "compile": "babel -d dist/ src/",
